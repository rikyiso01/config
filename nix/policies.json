{
    "policies": {
        "AppUpdateURL": "https://localhost",
        "DisableAppUpdate": true,
        "OverrideFirstRunPage": "",
        "OverridePostUpdatePage": "",
        "DisableSystemAddonUpdate": true,
        "DisableProfileImport": false,
        "DisableFirefoxStudies": true,
        "DisableTelemetry": true,
        "DisableFeedbackCommands": true,
        "DisablePocket": true,
        "DisableSetDesktopBackground": false,
        "DisableDeveloperTools": false,
        "DNSOverHTTPS": {
            "Enabled": true,
            "ProviderURL": "https://dns.mullvad.net/dns-query",
            "Locked": true,
            "Fallback": false
        },
        "NoDefaultBookmarks": true,
        "WebsiteFilter": {
            "Block": [
                "https://localhost/*"
            ],
            "Exceptions": [
                "https://localhost/*"
            ]
        },
        "Extensions": {
            "Install": [
                "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/darkreader/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/violentmonkey/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/file/4241113/rudolf_fernandes-2.2.9.xpi",
                "https://addons.mozilla.org/firefox/downloads/file/4274570/tab_numbering-0.1.2resigned1.xpi",
                "https://addons.mozilla.org/firefox/downloads/file/4259790/vimium_ff-2.1.2.xpi",
                "https://addons.mozilla.org/firefox/downloads/file/4285762/keepassxc_browser-1.9.0.5.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/floccus/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/file/4305354/video_downloadhelper-9.0.2.12.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/libredirect/latest.xpi"
            ],
            "Uninstall": [
                "google@search.mozilla.org",
                "bing@search.mozilla.org",
                "amazondotcom@search.mozilla.org",
                "ebay@search.mozilla.org",
                "twitter@search.mozilla.org"
            ]
        },
        "SearchEngines": {
            "PreventInstalls": false,
            "Add": [
              {
                "Name": "Brave",
                "URLTemplate": "https://search.brave.com/search?q={searchTerms}",
                "Method": "GET",
                "IconURL": "https://brave.com/leo-icons/social-brave-release-favicon-fullheight-color.svg",
                "Alias": "Brave",
                "Description": "Brave"
              }
            ],
            "Remove": [
                "Google",
                "Bing",
                "Amazon.com",
                "eBay",
                "Twitter"
            ],
            "Default": "Brave"
        },
        "SupportMenu": {
            "Title": "LibreWolf Issue Tracker",
            "URL": "https://codeberg.org/librewolf/issues"
        },
        "DisableBuiltinPDFViewer": true,
        "DisableFirefoxAccounts": true,
        "DisableFormHistory": true,
        "DisplayBookmarksToolbar": "never",
        "DisplayMenuBar": "never",
        "DontCheckDefaultBrowser": true,
        "DownloadDirectory": "${home}/Downloads",
        "EnableTrackingProtection": {
          "Value": true,
          "Locked": true,
          "Cryptomining": true,
          "Fingerprinting": true,
          "Exceptions": ["https://teams.microsoft.com"]
        },
        "FirefoxHome": {
          "Search": true,
          "TopSites": false,
          "SponsoredTopSites": false,
          "Highlights": false,
          "Pocket": false,
          "SponsoredPocket": false,
          "Snippets": false,
          "Locked": true
        },
        "FirefoxSuggest": {
          "WebSuggestions": false,
          "SponsoredSuggestions": false,
          "ImproveSuggest": false,
          "Locked": true
        },
        "Homepage": {
          "Locked": true,
          "StartPage": "none"
        },
        "HttpsOnlyMode": "force_enabled",
        "OfferToSaveLogins": true,
        "PasswordManagerEnabled": false,
        "PDFjs": {
          "Enabled": false
        }
    }
}
